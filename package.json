{
  "name": "elFinder",
  "version": "2.1.55a14",
  "description": "File manager for web",
  "main": "Jakefile.js",
  "scripts": {
    "build": "npx webpack --config webpack.config.js --mode production && mkdir -p ./elfinder_client && jake -C ./elfinder_client elfinder",
    "serve": "mkdir -p ./elfinder_client && nodemon --ext html,js --ignore ./js/proxy --ignore ./js/proxy --ignore ./elfinder_client --ignore ./node_modules --exec \"npx webpack --config webpack.config.js && jake -C ./elfinder_client serve\" --trace"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Studio-42/elFinder.git"
  },
  "keywords": [
    "file",
    "manager",
    "jquery",
    "jqueryui",
    "frontend",
    "client-side",
    "browser"
  ],
  "author": "Studio 42",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Studio-42/elFinder/issues"
  },
  "homepage": "http://studio-42.github.io/elFinder/",
  "devDependencies": {
    "copy-webpack-plugin": "^6.2.1",
    "csso": "<2.0.0",
    "jake": "~8.0.12",
    "nodemon": "^2.0.20",
    "uglify-js": "^3.13.10",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "browserfs": "^1.4.3",
    "content-disposition": "^0.5.4",
    "jimp": "^0.16.1",
    "jszip": "^3.7.0",
    "lzutf8": "^0.6.0",
    "mime": "^2.5.2",
    "serviceworker-webpack-plugin": "^1.0.1",
    "underscore": "^1.12.1"
  }
}
